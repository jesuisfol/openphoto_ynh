{
    "name": "OpenPhoto",
    "id": "openphoto",
    "description": {
        "en": "OpenPhoto, a photo service for the masses",
        "fr": "OpenPhoto, a photo service for the masses"
    },
    "developer": {
        "name": "Fol",
        "email": "fol@fol.tf",
        "url": "http://theopenphotoproject.org/"
    },
    "multi_instance": "true",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for OpenPhoto"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for OpenPhoto"
                },
                "example": "/openphoto",
                "default": "/openphoto"
            }
        ]
    }
}
